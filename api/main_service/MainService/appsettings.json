{
    "AllowedHosts": "*",
    "Cors": {
        "AllowedOrigins": [
            "http://localhost:3000",
            "https://avisapp.netlify.app",
            "https://staging-avis.netlify.app",
            "http://127.0.0.1:5173",
            "http://localhost:5173",
            "http://localhost"
        ]
    },
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "MongoDbSetting": {
        "DatabaseName": "avis-db"
    },
    "DbIndexs": {
        "Song": "pro_SongIndex",
        "Post": "pro_PostIndex"
    },
    "AWS": {
        "Region": "us-east-1"
    },
    "Services": {
        "Hum": "https://hum-avis.onrender.com/api"
    },
    "S3": {
        "SongsBucket": "pro.songs-bucket",
        "ResourcesBucket": "pro.resources-bucket"
    },
    "Jwt": {
        "Authority": "https://securetoken.google.com/auth-series"
    }
    
}
